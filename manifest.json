{
  "manifest_version": 2,

  "name": "Facebook Vs Codeforces Counter",
  "description": "This extension counts no of facebook/Codeforces visits",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ]
}